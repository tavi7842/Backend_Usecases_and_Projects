<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
   
    <bean class="com.bitlabs.Entity.Employee" name="emp">
    <property name="name">
    <value>Vinay</value>
    </property>
    <property name="city">
    <value>Guntur</value>
    </property>
    </bean>
    
    <!-- bean definitions here -->
    <bean class="com.bitlabs.Entity.Employee" name="emp1">
    <property name="name" value="Vinay"/>
    <property name="city" value="Guntur"/>
    </bean>
    
   
    
    
    
    <!-- bean definitions here -->
    <bean class="com.bitlabs.Entity.Employee" name="emp2"  p:name="Vinay" p:city="Guntur"/>
    
    
    
    

</beans>


   
   